<?xml version="1.0" encoding="UTF-8"?>
<nodes>
	<node type="raw" val="[{"/>
	<node type="switch">
		<nodes>
			<node type="case" left="item.Index" op="==" right="0">
				<nodes>
					<node type="raw" val="&quot;name&quot;: "/>
					<node type="tpl" val="item.Name"/>
					<node type="raw" val=","/>
				</nodes>
			</node>
			<node type="case" left="item.Index" op="==" right="1">
				<nodes>
					<node type="tpl" prefix="&quot;slug&quot;:" suffix="," val="item.Slug"/>
				</nodes>
			</node>
			<node type="case" left="item.Uid" op="==" right="-1">
				<nodes>
					<node type="raw" val="&quot;no_data&quot;: true"/>
				</nodes>
			</node>
		</nodes>
	</node>
	<node type="raw" val="}]"/>
</nodes>
